{
	"name": "@fluire/cli",
	"description": "Migration CLI helper for Fluire - Check our main package at https://npmjs.com/fluire",
	"version": "0.0.1",
	"type": "module",
	"main": "dist/index.js",
	"files": [
		"dist/",
		"README.md",
		"package.json"
	],
	"repository": {
		"type": "git",
		"directory": "packages/cli",
		"url": "https://github.com/fluiredev/fluire"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"author": {
		"name": "Matheus Landuci",
		"email": "matheuslanduci@gmail.com",
		"url": "https://matheuslanduci.com"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"start": "node dist/index.js"
	},
	"devDependencies": {
		"@types/node": "^20.11.17",
		"@vitest/coverage-istanbul": "2.1.8",
		"fluire": "^0.0.11",
		"tsup": "^8.3.5",
		"type-fest": "^4.30.2",
		"typescript": "^5.7.2",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^2.1.8"
	},
	"keywords": [
		"payments",
		"stripe",
		"payments-as-code",
		"pac",
		"fluire",
		"cli",
		"migration"
	],
	"dependencies": {
		"chalk": "^5.4.1",
		"commander": "^12.1.0",
		"esbuild": "^0.24.2"
	}
}
